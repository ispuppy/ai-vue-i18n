{
  "name": "ai-vue-i18n",
  "version": "1.0.8",
  "description": "基于AI的Vue项目自动化国际化工具，能够智能提取代码中的中文内容并生成多语言配置文件，简化Vue项目的i18n开发流程",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/ispuppy/ai-vue-i18n"
  },
  "bugs": {
    "url": "https://github.com/ispuppy/ai-vue-i18n/issues"
  },
  "homepage": "https://github.com/ispuppy/ai-vue-i18n#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "ai",
    "vue2",
    "vue3",
    "vue-i18n",
    "translate",
    "internationalization"
  ],
  "files": [
    "dist",
    "bin",
    "ai-vue-i18n.config.js",
    "README.md"
  ],
  "bin": {
    "i18n-translate": "./bin/generate.js",
    "i18n-config": "./bin/config.js",
    "i18n-check": "./bin/check.js",
    "i18n-add-pre-push": "./bin/pre-push.js"
  },
  "author": "puppy",
  "license": "ISC",
  "devDependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.4",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@vue/compiler-sfc": "^3.5.21",
    "@vue/test-utils": "^2.0.0",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "jest": "^29.0.0",
    "jiti": "^2.6.1",
    "magic-string": "^0.30.21",
    "rollup": "^4.50.0",
    "rollup-plugin-cleandir": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vue-eslint-parser": "^10.2.0"
  }
}
